    "@react-three/postprocessing": "^3.0.4",
    "@tailwindcss/vite": "^4.0.16",
    "@types/swiper": "^5.4.3",
    "aos": "^2.3.4",
    "framer-motion": "^12.4.11",
    "lottie-react": "^2.4.1",
    "prop-types": "^15.8.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
